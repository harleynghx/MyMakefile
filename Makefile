CC = gcc
AR = ar -rc
RM = rm -f
SRCS = 
BONUS =

			
CFLAGS = -Wall -Wextra -Werror
OPTION = -I ./

OBJS = $(SRCS:.c=.o)
BOBJS = $(BONUS:.c=.o)

